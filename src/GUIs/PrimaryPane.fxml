<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox prefHeight="768.0" prefWidth="1024.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="GUIs.PrimaryController">
    <children>
        <StackPane fx:id="sceneHolder" VBox.vgrow="ALWAYS">
            <children>
                <BorderPane prefHeight="200.0" prefWidth="200.0" stylesheets="PrimaryPane.css">
                    <right>
                        <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
                            <children>
                                <Button minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="25.0" prefWidth="109.0" text="Single Player" textAlignment="CENTER" VBox.vgrow="ALWAYS">
                                    <VBox.margin>
                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                    </VBox.margin>
                                </Button>
                                <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="126.0" text="Multiplayer" textAlignment="CENTER" VBox.vgrow="ALWAYS">
                                    <VBox.margin>
                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                    </VBox.margin>
                                </Button>
                                <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="140.0" text="Scenario Editor" textAlignment="CENTER" VBox.vgrow="ALWAYS">
                                    <VBox.margin>
                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                    </VBox.margin>
                                </Button>
                                <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="141.0" text="Preferences" textAlignment="CENTER" VBox.vgrow="ALWAYS">
                                    <VBox.margin>
                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                    </VBox.margin>
                                </Button>
                                <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="134.0" text="Exit" textAlignment="CENTER" VBox.vgrow="ALWAYS">
                                    <VBox.margin>
                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                    </VBox.margin>
                                </Button>
                            </children>
                        </VBox>
                    </right>
                    <top>
                        <TextField alignment="TOP_CENTER" text="Alaesia" BorderPane.alignment="CENTER">
                            <opaqueInsets>
                                <Insets top="-1.0" />
                            </opaqueInsets>
                            <BorderPane.margin>
                                <Insets bottom="100.0" top="100.0" />
                            </BorderPane.margin>
                        </TextField>
                    </top>
                </BorderPane>
            </children></StackPane>
    </children>
</VBox>
